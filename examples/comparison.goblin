# Integer comparisons
print(1 == 1)
print(1 == 2)
print(1 != 2)
print(1 < 2)
print(2 > 1)
print(1 <= 1)
print(1 >= 1)

# Float comparisons
print(1.5 < 2.5)
print(1.0 == 1.0)

# Cross-type int/float
print(1 < 1.5)
print(1.5 > 1)

# String comparisons
print("abc" == "abc")
print("abc" != "def")
print("abc" < "def")

# Bool comparisons
print(true == true)
print(true != false)

# Nil comparison
print(nil == nil)

# Comparison in if
if 10 > 5 {
    print("10 is greater")
}
